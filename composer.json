{
    "name": "quranacademy/b2-php-sdk",
    "description": "An SDK for working with B2 cloud storage",
    "keywords": ["b2", "storage", "backblaze", "cloud", "filesystem", "backup"],
    "homepage": "https://github.com/quranacademy/b2-php-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Muhammad Imangazaliev",
            "email": "imangazalievm@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Backblaze\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backblaze\\Tests\\": "tests/"
        }
    }
}
